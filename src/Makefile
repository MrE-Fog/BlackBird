#------------------------------------------------------------------------------
# all:
#------------------------------------------------------------------------------

all:
	gcc $(CFLAGS) BlackBird.c $(LFLAGS) -o ../bin/bb

#------------------------------------------------------------------------------
# clean:
#------------------------------------------------------------------------------

clean:
	rm -f ../bin/bb

#------------------------------------------------------------------------------
# install:
#------------------------------------------------------------------------------

install:
	mkdir -p $(bin-dir)
	install -m755 ../bin/bb $(bin-dir)
